{
  "cli": {
    "version": ">= 16.5.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_DEBUG": "true"
      },
      "extends": "base"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "buildType": "apk"
      },
      "extends": "base"
    },
    "base": {
      "node": "18.17.0",
      "yarn": "1.22.19",
      "resourceClass": "medium"
    }
  },
  "submit": {
    "production": {}
  }
}
